#tag WindowCode
	#tag Event
		Sub Open()
		  
		  
		  
		  
		  me.BackColor = get_color("Gray",0)
		  
		  
		  scripts = new scripts_class
		  
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub LoadMe()
		  
		  
		  PopulateLabels
		  PopulateFieldNames
		  
		  If ContactOrVenue ="" Or ContactVenuePKID = "" Or FieldNames.Ubound = -1 Then
		    Return
		  End If
		  
		  
		  Dim TableName as string
		  Dim exre1 as ExecuteReturn
		  Dim TheRecordSet as RecordSet
		  Dim db1 as otis_database_manager = app.otis_db
		  
		  
		  
		  Select Case ContactOrVenue
		  Case "Contact"
		    TableName = "contacts_"
		  Case "Venue"
		    TableName = "venues_"
		  End Select
		  
		  
		  exre1 = db1.execute("Select",TableName,FieldNames(),Array(""),Array("pkid = '" + ContactVenuePKID + "'") )
		  
		  If exre1.TheRecordSet = Nil Then
		    Return
		  End If
		  
		  TheRecordSet = exre1.TheRecordSet
		  
		  For i1 as integer = 0 To FieldNames.Ubound
		    sdoTextFieldControl(i1).Text = TheRecordSet.Field(FieldNames(i1)).StringValue
		  Next
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Function NewContactVenue() As String
		  Dim exre1 as ExecuteReturn
		  Dim db1 as otis_database_manager = app.otis_db
		  Dim rs1 as RecordSet
		  
		  
		  
		  
		  me.PopulateLabels
		  me.PopulateFieldNames
		  me.ShowModal
		  
		  
		  // User interactionHere
		  
		  If UserAbort Then
		    Return ""
		  End If
		  
		  // User Done interacting
		  Dim TableName,Columns(),Values() as string
		  
		  Select Case ContactOrVenue
		  Case "Contact"
		    TableName = "contacts_"
		  Case "Venue"
		    TableName = "venues_"
		  End Select
		  
		  For i1 as integer = 0 To FieldNames.Ubound
		    ReDim Values(i1)
		    values(i1) = "'" +  sdoTextFieldControl(i1).Text + "'"
		  Next
		  Columns = FieldNames()
		  
		  exre1 = db1.execute("Insert",TableName,Columns(),Values(),Array("") )
		  
		  me.Close
		  
		  Return exre1.ThePKID
		End Function
	#tag EndMethod

